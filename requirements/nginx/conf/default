server {
        listen 443 ssl default_server;
        listen [::]:443 ssl default_server;

        root /var/www/html/mr;

        index index.html;

        server_name _;  

        ssl_certificate /etc/ssl/certs/nginx.crt;
        ssl_certificate_key /etc/ssl/private/nginx.key;

        location / {
                try_files $uri $uri/ =404;
        }
}

